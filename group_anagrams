answer:
class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        dicte={}
        for char in strs:
            key="".join(sorted(char))
            if key in dicte:
                dicte[key].append(char)
            else:
                dicte[key]=[char]
        return list(dicte.values())

