class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        index={}
        left=0
        max_length=0
        for right in range(len(s)):
            if s[right] in index and index[s[right]]>=left:
                left=index[s[right]]+1
            index[s[right]]=right
            max_length=max(max_length,right-left+1)
        return max_length
            


